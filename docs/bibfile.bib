@article{Castro2008,
abstract = {Os aut{\^{o}}matos celulares s{\~{a}}o ferramentas que podem representar quase todos os sistemas evolutivos que se pode imaginar. Suas principais caracter{\'{i}}sticas s{\~{a}}o a computa{\c{c}}{\~{a}}o descentralizada, onde cada c{\'{e}}lula {\'{e}} capaz de computar sua evolu{\c{c}}{\~{a}}o apenas baseando-se nos estados anteriores do sistema de c{\'{e}}lulas, e a simplicidade de suas regras, que, no entanto dificilmente podem ter suas conse- q{\"{u}}{\^{e}}ncias previstas. Atualmente pesquisadores de v{\'{a}}rios diferentes campos utilizam modelos de aut{\^{o}}matos celulares para simular diferentes tipos de aplica{\c{c}}{\~{o}}es na biologia evolutiva, na din{\^{a}}mica das rea{\c{c}}{\~{o}}es qu{\'{i}}micas, nos sistemas din{\^{a}}micos da f{\'{i}}sica, no com- portamento de mercados e muitas outras. Todas as aplica{\c{c}}{\~{o}}es hoje existentes s{\~{a}}o de alguma forma baseadas nos trabalhos de John von Neumann, John Holton Conway e Stephen Wolfram, cujas implementa{\c{c}}{\~{o}}es ser{\~{a}}o abordadas neste trabalho.},
author = {Castro, Marcia Luciana Aguena and Castro, Reinaldo De Oliveira},
issn = {1980-1793},
journal = {Revista de Ci{\^{e}}ncias Exatas e Tecnologia},
keywords = {Aut{\^{o}}matos celulares,Princ{\'{i}}pio da Equi-,ista de ci{\^{e}}ncias exatas,jogo da vida,val{\^{e}}ncia Computacional.},
number = {3},
pages = {89--106},
title = {{Aut{\^{o}}matos celulares: implementa{\c{c}}{\~{o}}es de von Neumann, Conway e Wolfram}},
volume = {3},
year = {2008}
}

@article{White2007,
abstract = {The main goal of this work is to introduce a theoretical model, based on cellular automata, to simulate epidemic spreading. Specifically, it divides the population into three classes: susceptible, infected and recovered, and the state of each cell stands for the portion of these classes of individuals in the cell at every step of time. The effect of population vaccination is also considered. The proposed model can serve as a basis for the development of other algorithms to simulate real epidemics based on real data. {\textcopyright} 2006 Elsevier Inc. All rights reserved.},
author = {White, S. Hoya and del Rey, A. Mart{\'{i}}n and S{\'{a}}nchez, G. Rodr{\'{i}}guez},
doi = {10.1016/j.amc.2006.06.126},
issn = {00963003},
journal = {Applied Mathematics and Computation},
keywords = {Cellular automata,Epidemic spreading,Mathematical modeling,Rectangular lattices,SIR model},
number = {1},
pages = {193--202},
title = {{Modeling epidemics using cellular automata}},
volume = {186},
year = {2007}
}

